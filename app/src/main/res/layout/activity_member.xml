<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/confirm_member"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile"
    android:onClick="onClick"
    tools:context=".ui.mypage.MemberActivity"
    tools:ignore="DuplicateIds">

    <LinearLayout
        android:layout_width="137dp"
        android:layout_height="301dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/name_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="이름"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/id_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="아이디"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/email_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="이메일"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/address_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="주소"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/birth_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="생년월일"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/phone_info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="전화번호"
            android:textSize="17.2sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="137dp"
        android:layout_height="301dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.188"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/name_info1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="이름"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/id_info1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="아이디"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/email_info1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="이메일"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/address_info1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="주소"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/birth_info1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="생년월일"
            android:textSize="17.2sp" />

        <TextView
            android:id="@+id/phone_info1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="12dp"
            android:paddingBottom="8.4dp"
            android:text="전화번호"
            android:textSize="17.2sp" />

    </LinearLayout>

    <Button
        android:id="@+id/confirmbutton"
        android:layout_width="320dp"
        android:layout_height="51dp"
        android:layout_marginBottom="143dp"
        android:text="확인하기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
